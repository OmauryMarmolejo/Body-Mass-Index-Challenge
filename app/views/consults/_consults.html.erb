<div class="row center">
  <h4 class="">History</h4>
  <hr>
    <div>
      <table class="table table-striped table-hover">
        <thead>
          <tr>
            <th>Height</th>
            <th>Weight</th>
            <th>BMI</th>
            <th>Date</th>
            <th>Delete</th>
          </tr>
        </thead>
        <tbody>
        <% @consults.each do |consult| %>
          <tr>
            <td><%= consult.height %> cm</td>
            <td><%= consult.mass %> kg</td>
            <td><%= "%.2f" % consult.body_mass %></td>
            <td><%= consult.created_at.strftime("%Y-%m-%d")%></td>
            <td> <%= link_to "Delete", root_path(@consult), method: :delete, data: {confirm: "Really delete the consult?"} %></td>
          </tr>
        <% end %>
    </div>
</div>
