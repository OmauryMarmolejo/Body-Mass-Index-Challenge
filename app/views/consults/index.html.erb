<div class="container">
  <div class="col-sm-10 col-sm-offset-1 col-xs-12 blog-content">
    <% @consults.each do |consult| %>
      <div class="col-xs-12">
        <div class="text-center">
          <%= consult.height %>
        </div>
        <div>
          <%= consult.mass %>
          <%= link_to "Delete", root_path(@consult), method: :delete, data: {confirm: "Really delete the consult?"} %>
        </div>
        <br>
      </div>
    <% end %>
   </div>
</div>
